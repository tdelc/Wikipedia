                            labels: ["2008","2009","2010","2011","2012","2013","2014","2015","2016","2017","2018","2019","2020","2021","2022","2023","2024","2025"],
                            datasets: yearDatasets,
                        },
                        options: {
                            responsive: true,
                            legend: {
                                display: false,
                            },
                            tooltips: {
                                mode: 'label',
                                callbacks: {
                                    label: function(tooltipItem) {
                                        return yearDatasets[tooltipItem.datasetIndex].label + ': '
                                            + (new Number(tooltipItem.yLabel)).toLocaleString(i18nLang);
                                    }
                                }
                            },
                            barValueSpacing: 20,
                            scales: {
                                yAxes: [{
                                    id: 'edits',
                                    type: 'linear',
                                    position: 'left',
                                    gridLines:{
                                        display: false
                                    },
                                    scaleLabel: {
                                        display: true,
                                        labelString: "Edits"
                                    },
                                    ticks: {
                                        beginAtZero: true,
                                        callback: function (value) {
                                            if (Math.floor(value) === value) {
                                                return value.toLocaleString(i18nLang);
                                            }
                                        }
                                    },
                                    gridLines: {
                                        color: xtools.application.chartGridColor
                                    }
                                }, {
                                    id: 'size',
                                    type: 'linear',
                                    position: 'right',
                                    gridLines:{
                                        display: false
                                    },
